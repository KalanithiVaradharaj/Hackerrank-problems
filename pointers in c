#include <stdio.h>

void update(int *a,int *b) {
    int c = (*b);
    int d = (*a);
    int f;
    if (c<d){
        f = d-c;
    }
    else{
        f = c-d;
    }
    printf("%d\n%d", c+d, f);
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    return 0;
}
